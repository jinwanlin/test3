<h1>Listing search_histories</h1>

<table>
  <tr>
    <th>User</th>
    <th>Keywords</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @search_histories.each do |search_history| %>
  <tr>
    <td><%= search_history.user %></td>
    <td><%= search_history.keywords %></td>
    <td><%= link_to 'Show', search_history %></td>
    <td><%= link_to 'Edit', edit_search_history_path(search_history) %></td>
    <td><%= link_to 'Destroy', search_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

